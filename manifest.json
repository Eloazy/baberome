{
	"name": "Baberome",
	"description": "browser environment branch from babel",
	"version": "1.0.0",
	"manifest_version": 3,
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self'",
    	"sandbox": "sandbox allow-scripts allow-forms"
	},
	"permissions": [
		"activeTab"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"action": {
		"default_popup": "main.html",
		"default_icon": "baberome.png"
	}
}